{
  "name": "sanity-plugin-documents-pane",
  "version": "2.1.0",
  "description": "Displays the results of a GROQ query in a View Pane",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-documents-pane#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-documents-pane/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-documents-pane.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./lib/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build --strict",
    "clean": "rimraf lib",
    "compile": "tsc --noEmit",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run compile && npm run build",
    "watch": "pkg-utils watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "dependencies": {
    "@sanity/icons": "^2.0.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.0",
    "@sanity/util": "^3.0.0",
    "@sanity/uuid": "^3.0.1",
    "dlv": "^1.1.3",
    "react-fast-compare": "^3.2.0",
    "rxjs": "^6.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@sanity/pkg-utils": "^2.4.7",
    "@sanity/plugin-kit": "^2.2.0",
    "@sanity/semantic-release-preset": "^4.1.3",
    "@types/dlv": "^1.1.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "sanity": "^3.15.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
